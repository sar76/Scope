{
  "name": "scope-dom-extraction",
  "version": "1.0.0",
  "description": "Chrome extension for extracting and analyzing UI elements from web pages",
  "main": "index.js",
  "scripts": {
    "build": "webpack --mode=production",
    "build:dev": "webpack --mode=development",
    "watch": "webpack --mode=development --watch",
    "clean": "rm -rf build/*",
    "lint": "eslint src/**/*.js",
    "lint:fix": "eslint src/**/*.js --fix",
    "type-check": "tsc --noEmit",
    "test": "echo \"No tests specified\" && exit 0",
    "dev": "npm run build:dev && npm run watch"
  },
  "keywords": [
    "chrome-extension",
    "dom-extraction",
    "ui-analysis",
    "web-scraping",
    "accessibility"
  ],
  "author": "Scope Team",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.23.0",
    "@babel/preset-env": "^7.23.0",
    "@types/chrome": "^0.0.254",
    "babel-loader": "^9.1.3",
    "copy-webpack-plugin": "^11.0.0",
    "css-loader": "^6.8.1",
    "dotenv": "^16.5.0",
    "eslint": "^8.0.0",
    "html-webpack-plugin": "^5.5.3",
    "style-loader": "^3.3.3",
    "terser-webpack-plugin": "^5.3.9",
    "typescript": "^5.0.0",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/your-org/scope-dom-extraction.git"
  },
  "bugs": {
    "url": "https://github.com/your-org/scope-dom-extraction/issues"
  },
  "homepage": "https://github.com/your-org/scope-dom-extraction#readme",
  "dependencies": {
    "@tensorflow-models/universal-sentence-encoder": "^1.3.3",
    "@tensorflow/tfjs": "^4.16.0",
    "jszip": "^3.10.1"
  }
}
